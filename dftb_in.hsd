Geometry = xyzFormat {
<<< "in.xyz"
}

Hamiltonian = DFTB {
SCC = yes
SCCTolerance = 1.0e-5
Charge = 0.0

MaxSCCIterations = 500

MaxAngularMomentum = {
#      C = 'p'
#      H = 's'
#      N = 'p'
#      O = 'p'
}

SlaterKosterFiles = Type2FileNames {
        Prefix = "/path/to/SlaterKoser"
        Separator = ""
        Suffix = "-c.spl"
        LowerCaseTypeName = Yes
        }

}

Options = {
WriteDetailedXML = Yes
}

Analysis = {
WriteEigenvectors = Yes
}



